{% extends 'base.html.twig' %}

{% block title %}Intervenant{% endblock %}

{% block body %}
    <h1>Intervenant</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ intervenant.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ intervenant.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ intervenant.prenom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ intervenant.adresse }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ intervenant.email }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ intervenant.telephone }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ intervenant.createdAt ? intervenant.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedBy</th>
                <td>{{ intervenant.createdBy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_intervenant_index') }}">back to list</a>

    <a href="{{ path('app_intervenant_edit', {'id': intervenant.id}) }}">edit</a>

    {{ include('intervenant/_delete_form.html.twig') }}
{% endblock %}
